<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Films">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Film">
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:int" name="id"/>
              <xs:element type="xs:string" name="title"/>
              <xs:element type="xs:int" name="year"/>
              <xs:element type="xs:string" name="direcor"/>
              <xs:element type="xs:string" name="stars"/>
              <xs:element type="xs:string" name="review"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>